<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="thesis_application.sync_page"
             Title="Szinkronizáció"
             >

    <!-- SYNCRONIZATION PAGE -->

    <RefreshView
        x:Name="refresh_container"
        Command="{Binding refresh}"
        >
        <ScrollView>
            <StackLayout
                x:Name="main_container"
                Padding="12"
                >
            </StackLayout>
        </ScrollView>
    </RefreshView>

</ContentPage>